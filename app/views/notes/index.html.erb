<h1>Listing notes</h1>

<table>
  <tr>
    <th>Board</th>
    <th>User</th>
    <th>Title</th>
    <th>Priority</th>
    <th>Outcome</th>
    <th>Working</th>
    <th>Problem</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.board_id %></td>
    <td><%= note.user_id %></td>
    <td><%= note.title %></td>
    <td><%= note.priority %></td>
    <td><%= note.outcome %></td>
    <td><%= note.working %></td>
    <td><%= note.problem %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Note', new_note_path %>
